#include <iostream>
#include <string>

using namespace std;


char letterstext[26] = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z'};
string lettersbinary[26] = {"00001", "00010", "00011", "00100", "00101", "00110", "00111", "01000", "01001", "01010", "01011", "01100", "01101", "01110", "01111", "10000", "10001", "10010", "10011", "10100", "10101", "10110", "10111", "11000", "11001", "11010"};



int main() {
    string text,letters,i;
    cout << "Welcome to Text-to-Binary translator! Enter your text: " << endl;
    getline(cin, text); 
    for (char &c : text) c = tolower(c);  
    for (int i = 0; i < text.length(); i++) {
    char current = text[i];

    for (int j = 0; j < 26; j++) {
        if (current == letterstext[j]) {
            cout << lettersbinary[j] << " ";
            break;
        }
    }


    

  

}

    return 0;
}